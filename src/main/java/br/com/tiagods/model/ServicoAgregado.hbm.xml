<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="ServicoAgregado" table="SERVICO_AGREGADO">
	<id name="id" column="SER_AGR_COD" type="int">
	<generator class="identity" />
	</id>
	<property name="nome" column="SER_AGR_NOME" length="100"/>
	
	<set name="servicosContratados" inverse="true" cascade="save-update, delete">
		<key column="SER_CON_SERVICOAGREGADO_COD"/>
		<one-to-many class="ServicoContratado"/>
	</set>
	
	</class>
</hibernate-mapping>

