<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="Departamento" table="USUARIO_DEPARTAMENTO">
	<id name="id" column="USU_DEP_COD" type="int">
	<generator class="sequence">
		<param name="sequence">USUARIO_DEPARTAMENTO_DEP_COD_SEQUENCE</param>
	</generator>
	</id>
	<property name="nome" column="USU_DEP_NOME" length="100"/>
	</class>
</hibernate-mapping>

