<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="UsuarioAcesso" table="USUARIO_ACESSO">
	<id name="id" column="USU_ACE_COD" type="int">
	<generator class="identity"/>
	</id>
	<property name="data" column="USU_ACE_DATA" type="timestamp"/>
	<many-to-one name="usuario" column="USU_ACE_USUARIO_ID" foreign-key="FK_USU_ACE_USUARIO_ID"></many-to-one>
	<property name="maquina" column="USU_ACE_MAQUINA" length="100"/>
	</class>
</hibernate-mapping>

