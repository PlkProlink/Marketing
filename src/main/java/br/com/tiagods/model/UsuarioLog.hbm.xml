<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="UsuarioLog" table="USUARIO_LOG">
	<id name="id" column="USU_LOG_COD" type="int">
	<generator class="identity"/>
	</id>
	<property name="data" column="USU_LOG_DATA" type="timestamp"/>
	<many-to-one name="usuario" column="USU_LOG_USUARIO_ID" foreign-key="FK_USU_LOG_USU_ID"></many-to-one>
	<property name="menu" column="USU_LOG_MENU" length="50"/>
	<property name="acao" column="USU_LOG_ACAO" length="50"/>
	<property name="descricao" column="USU_LOG_DESCRICAO"/>
	<property name="maquina" column="USU_LOG_MAQUINA" length="100"/>
	</class>
</hibernate-mapping>

