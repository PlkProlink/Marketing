<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="Origem" table="ORIGEM">
	<id name="id" column="ORI_COD" type="int">
	<generator class="sequence">
		<param name="sequence">ORIGEM_ORI_COD_SEQUENCE</param>
	</generator>
	</id>
	<property name="nome" column="ORI_NOME" length="100"/>
	</class>
</hibernate-mapping>

