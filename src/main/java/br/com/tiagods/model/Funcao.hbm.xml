<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<!-- Tiago Dias
-->
<hibernate-mapping package="br.com.tiagods.model">
	<class name="Funcao" table="FUNCAO">
	<id name="id" column="FUN_COD" type="int">
	<generator class="sequence">
		<param name="sequence">FUNCAO_FUN_COD_SEQUENCE</param>
	</generator>
	</id>
	<property name="nome" column="FUN_NOME" length="100"/>
	</class>
</hibernate-mapping>

